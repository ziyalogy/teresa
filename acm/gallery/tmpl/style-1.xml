<?xml version="1.0" encoding="utf-8"?>
<form>
	<title>Gallery Style 1</title>
	<sampledata></sampledata>
    <fields name="isotope">
		<fieldset name="basic-1" label="ISOTOPE_BASIC1_LABEL" description="ISOTOPE_BASIC1_DESC">
			<field name="colmb" type="list" label="ISOTOPE_COL_MOBILE_LABEL" default="" description="ISOTOPE_COL_MOBILE_DESC">
				<option value="1">1</option>
				<option value="2">2</option>
				<option value="3">3</option>
				<option value="4">4</option>
				<option value="5">5</option>
				<option value="6">6</option>
				<option value="7">7</option>
				<option value="8">8</option>
				<option value="9">9</option>
				<option value="10">10</option>
			</field>
			
			<field name="coltb" type="list" label="ISOTOPE_COL_TABLET_LABEL" default="" description="ISOTOPE_COL_TABLET_DESC">
				<option value="1">1</option>
				<option value="2">2</option>
				<option value="3">3</option>
				<option value="4">4</option>
				<option value="5">5</option>
				<option value="6">6</option>
				<option value="7">7</option>
				<option value="8">8</option>
				<option value="9">9</option>
				<option value="10">10</option>
			</field>
			
			<field name="coldt" type="list" label="ISOTOPE_COL_DESKTOP_LABEL" default="" description="ISOTOPE_COL_DESKTOP_DESC">
				<option value="1">1</option>
				<option value="2">2</option>
				<option value="3">3</option>
				<option value="4">4</option>
				<option value="5">5</option>
				<option value="6">6</option>
				<option value="7">7</option>
				<option value="8">8</option>
				<option value="9">9</option>
				<option value="10">10</option>
			</field>
			
			<field name="gutter" type="text" default="0" label="ISOTOPE_GUTTER_LABEL" description="ISOTOPE_GUTTER_DESC" filter="integer"></field>
			
		</fieldset>
		
    <fieldset name="basic-2" label="ISOTOPE_BASIC2_LABEL" description="ISOTOPE_BASIC2_DESC" multiple="0">
			<field name="gallery" type="jalist">
				<item name="selectitem" type="list" default="1" label="ISOTOPE_SIZE_ITEMS_LABEL"
										 description="ISOTOPE_SIZE_ITEMS_DESC">
										<option value="1">Item 1x1</option>
										<option value="2">Item 2x2</option>
				</item>
				<item name="img" type="media" label="ISOTOPE_IMG_LABEL" description="ISOTOPE_IMG_DESC" required="1"/>

				<item name="alt" type="text" label="ISOTOPE_IMG_ALT_LABEL" description="ISOTOPE_IMG_ALT_DESC" required="1"/>

			</field>
    </fieldset>
    </fields>
</form>